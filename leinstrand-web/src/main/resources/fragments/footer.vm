<script type="text/javascript">
    jQuery(document).ready(function() {
        App.init();
//        App.initSliders();
#if($thisPage.getTemplate() == "contact")
        Contact.initMap();
#end

        $('#redactor_save').hide();
        $('#redactor_edit').click(function(e) {
            e.preventDefault();
#foreach($id in $redactorIdList)
            $('#${id}').redactor({
                paragraphy: false,
                convertDivs: false,
                convertLinks: true,
                convertImageLinks: true,
                convertVideoLinks: true,
                removeEmptyTags: false,
                formattingPre: true,
                linkAnchor: true,
                linkEmail: true,
                tidyHtml: false,
                minHeight: 320,
                tabSpaces: 4,
                wym: true
            });
#end
#foreach($id in $redactorAirIdList)
            $('#${id}').redactor({
                air: true,
                paragraphy: false,
                convertDivs: false,
                convertLinks: false,
                pastePlainText: true
            });
#end
            $(this).hide();
            $('#redactor_save').show();
        });

        $('#redactor_save').click(function(e) {
            e.preventDefault();
#foreach($id in $redactorIdList)
            $.ajax({
                url: '/api/save/textnode',
                type: 'post',
                data: 'id=${id}&content=' + $('#${id}').redactor('get')
            });
            $('#${id}').redactor('destroy');
#end
#foreach($id in $redactorAirIdList)
            var $data = $('#${id}').redactor('get');
            $.ajax({
                url: '/api/save/textnode',
                type: 'post',
                data: 'id=${id}&content=' + $('#${id}').redactor('get')
            });
            $('#${id}').redactor('destroy');
#end
            location.reload(true);
        });

    });
</script>
